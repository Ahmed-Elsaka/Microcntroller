#include "STD_TYPES.h"
#include "LED_interface.h"
#include "SWI_interface.h"
#include <util/delay.h>


int main(){
	DIO_voidInit();
	u8 Local_SWI_state = -1;
	while(1){
		SWI_u8GetSwitchState(1 ,&Local_SWI_state);


		if(Local_SWI_state==1){
			LED_u8SetLedOff(5);
		}else{
			LED_u8SetLedOn(5);
		}

	}

	return 0 ;
}

